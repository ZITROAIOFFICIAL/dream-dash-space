      <video 
        id="hero-video"
        class="hero-video"
        autoplay 
        loop 
        muted 
        playsinline
        webkit-playsinline
        preload="auto"
        defaultMuted
        controlslist="nodownload nofullscreen noplaybackrate"
        disablepictureinpicture
