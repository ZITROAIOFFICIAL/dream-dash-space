            </video>

            <script>
            (function() {
              const video = document.getElementById('global-payment-video');
              const image = document.getElementById('global-payment-image');
              
              async function detectPowerSavingMode() {
                let isPowerSaving = false;
                
                // 1. Vérifier la préférence de mouvement réduit
                if (window.matchMedia('(prefers-reduced-motion: reduce)').matches) {
                  isPowerSaving = true;
                }
                
                // 2. Vérifier la Battery API (si disponible)
                if ('getBattery' in navigator) {
                  try {
                    const battery = await navigator.getBattery();
                    // Mode économie si batterie < 20% ou si en charge avec niveau bas
                    if (battery.level < 0.2 || (battery.charging && battery.level < 0.5)) {
                      isPowerSaving = true;
                    }
                  } catch (e) {
                    console.log('Battery API non disponible');
                  }
                }
                
                // 3. Vérifier si la vidéo peut jouer (détection indirecte du mode économie)
                const canPlay = await new Promise((resolve) => {
                  video.play().then(() => resolve(true)).catch(() => resolve(false));
                });
                
                if (!canPlay) {
                  isPowerSaving = true;
                }
                
                return isPowerSaving;
              }
              
              // Appliquer la logique d'affichage
              detectPowerSavingMode().then(isPowerSaving => {
                if (isPowerSaving) {
                  // Mode économie d'énergie : afficher l'image uniquement
                  video.style.display = 'none';
                  video.pause();
                  image.style.display = 'block';
                } else {
                  // Mode normal : afficher la vidéo
                  video.style.display = 'block';
                  image.style.display = 'none';
                  video.play().catch(() => {
                    // Si la vidéo ne peut pas jouer, afficher l'image
                    video.style.display = 'none';
                    image.style.display = 'block';
                  });
                }
              });
            })();
            </script>
