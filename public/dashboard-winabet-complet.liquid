      // DÃ©cide alÃ©atoirement si Ã§a monte ou descend (50/50)
      const goingUp = Math.random() > 0.5;
      
      let variation;
      if (goingUp) {
        // Monte entre minIncrease et maxIncrease
        const range = membersConfig.maxIncrease - membersConfig.minIncrease;
        variation = membersConfig.minIncrease + Math.floor(Math.random() * (range + 1));
        console.log('â¬†ï¸ MontÃ©e de:', variation, `(entre ${membersConfig.minIncrease} et ${membersConfig.maxIncrease})`);
      } else {
