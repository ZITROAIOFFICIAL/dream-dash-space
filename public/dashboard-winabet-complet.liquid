              let base = parseFloat(computed.fontSize) || 16;
              el.style.fontSize = base + 'px';
              let tries = 0;
              while ((el.scrollWidth > box.clientWidth || el.scrollHeight > box.clientHeight) && base > 8 && tries < 30) {
                base -= 1;
                el.style.fontSize = base + 'px';
                tries++;
              }
