    .nav-item.active {
      background: rgba(22, 163, 74, 0.2);
      border: 2px solid #16a34a;
      color: #16a34a;
      font-weight: 600;
    }

    .nav-icon {
      width: 1.75rem;
      height: 1.75rem;
      flex-shrink: 0;
    }

    .nav-icon-small {
